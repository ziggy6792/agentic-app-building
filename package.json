{
  "private": true,
  "name": "zc",
  "author": "Simon Verhoeven",
  "scripts": {
    "dev": "turbo run dev",
    "build": "turbo run build",
    "deploy:dev": "ENV=dev turbo run deploy && ENV=dev pnpm sst:deploy",
    "deploy:prod": "ENV=prod turbo run deploy && ENV=prod pnpm sst:deploy",
    "sst:deploy": "pnpm --filter @zc/sst-app deploy:env",
    "build:prod": "ENV=prod turbo run build",
    "build:dev": "sst:shell -- ENV=dev turbo run build",
    "build:deploy": "run-s build deploy",
    "lint": "turbo run lint",
    "typecheck": "turbo run typecheck",
    "validate": "turbo run validate",
    "validate:check": "turbo run validate:check",
    "test": "turbo run test",
    "test:coverage": "turbo run test:coverage",
    "format": "prettier --write ."
  },
  "devDependencies": {
    "eslint": "^8.57.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.3.3",
    "ts-node": "^10.9.2",
    "tsconfig": "*",
    "turbo": "^2.1.2",
    "typescript": "^5.5.4"
  },
  "packageManager": "pnpm@10.17.1+sha512.17c560fca4867ae9473a3899ad84a88334914f379be46d455cbf92e5cf4b39d34985d452d2583baf19967fa76cb5c17bc9e245529d0b98745721aa7200ecaf7a"
}
