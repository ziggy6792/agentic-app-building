{
  "name": "@zc/sst-app",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "deploy:env": "pnpm clean && sst deploy --stage $ENV --yes",
    "clean": "chmod -R 777 ./.sst && rm -rf ./.sst || echo skip delete",
    "sst:shell": "sst shell",
    "lint:check": "NODE_ENV=production eslint \"./**/*.ts\" --report-unused-disable-directives --max-warnings 0",
    "lint": "pnpm lint:check --fix",
    "format:check": "prettier --ignore-path '../../.prettierignore' --check .",
    "validate:check": "run-s format:check"
  },
  "dependencies": {
    "@pulumi/cdk": "^1.10.0",
    "@pulumi/docker": "^4.8.0",
    "@pulumi/pulumi": "^3.178.0",
    "dotenv": "^17.2.3",
    "esbuild-plugin-tsc": "^0.4.0",
    "sst": "3.17.6"
  },
  "devDependencies": {
    "@zc/eslint-config": "workspace:*",
    "@zc/tsconfig": "workspace:*",
    "@types/eslint": "^8.56.10",
    "@typescript-eslint/eslint-plugin": "^8.46.1",
    "@typescript-eslint/parser": "^8.46.1",
    "eslint": "^8.57.1",
    "eslint-config-next": "^14.2.33",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.0.0",
    "@zc/web-app": "workspace:*",
    "if-env": "^1.0.4",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.3.3",
    "rimraf": "^6.0.1",
    "typescript": "^5.5.4"
  },
  "ct3aMetadata": {
    "initVersion": "7.37.0"
  }
}
